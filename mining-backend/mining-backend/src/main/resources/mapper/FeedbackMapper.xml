<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.slb.mining_backend.modules.system.mapper.FeedbackMapper">
    <insert id="insert" parameterType="com.slb.mining_backend.modules.system.entity.Feedback" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO feedback (user_id, type, content, contact, create_time)
        VALUES (#{userId}, #{type}, #{content}, #{contact}, NOW())
    </insert>
</mapper>
